<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartcarparking">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gglogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gglogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DirectionOnMap"></activity>
        <activity android:name=".ParkingSlotDetail" />
        <activity android:name=".ViewParkingSlots" />
        <activity android:name=".PayAndRateUser" />
        <activity android:name=".PaymentMethod" />
        <activity android:name=".EndCurrentParking" />
        <activity android:name=".AddParking" />
        <activity android:name=".ServiceMain" />
        <activity android:name=".ParkingOwnerList" />
        <activity android:name=".CompleteBookingDetail" />
        <activity android:name=".ForgetPassword" />
        <activity
            android:name=".CompleteProfileActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".SignUpActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
    </application>

</manifest>